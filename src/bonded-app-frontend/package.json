{
  "name": "bonded-app-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "setup": "npm i && dfx canister create bonded-app-backend && dfx generate bonded-app-backend && dfx deploy",
    "dev": "vite --port 3000 --host",
    "start": "node scripts/start.js",
    "prebuild": "echo 'Starting optimized build process...'",
    "build": "npm run build:fast",
    "build:fast": "NODE_OPTIONS='--max-old-space-size=2048' vite build --mode development || (test -d dist && echo 'Build succeeded despite warnings' && exit 0)",
    "build:optimized": "NODE_OPTIONS='--max-old-space-size=3072' vite build",
    "build:no-ai": "NODE_OPTIONS='--max-old-space-size=1536' EXCLUDE_AI_MODELS=true vite build",
    "build:ultra-light": "NODE_OPTIONS='--max-old-space-size=1024 --max-semi-space-size=64' EXCLUDE_AI_MODELS=true vite build --mode development",
    "build:help": "node scripts/build-helper.js --help",
    "build:laptop": "node scripts/laptop-friendly-build.js",
    "deploy:playground": "node scripts/deploy-helper.js --playground",
    "deploy:help": "node scripts/deploy-helper.js --help",
    "vercel-build": "npm run build:fast",
    "format": "prettier --write \"src/**/*.{json,js,jsx,ts,tsx,css,scss}\"",
    "generate-pwa-assets": "pwa-asset-generator public/images/app-icon.svg public/images --icon-only --favicon --opaque false --padding 0"
  },
  "dependencies": {
    "@dfinity/agent": "^2.1.3",
    "@dfinity/auth-client": "^2.4.1",
    "@dfinity/candid": "^2.1.3",
    "@dfinity/principal": "^2.1.3",
    "@xenova/transformers": "^2.4.0",
    "buffer": "^6.0.3",
    "core-js": "^3.42.0",
    "crypto-browserify": "^3.12.1",
    "crypto-js": "^4.2.0",
    "idb": "^7.1.1",
    "jspdf": "^3.0.1",
    "jszip": "^3.10.1",
    "nsfwjs": "^4.2.1",
    "onnxruntime-web": "^1.16.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "react-select": "^5.10.1",
    "stream-browserify": "^3.0.0",
    "telegram": "^2.26.22",
    "tesseract.js": "^6.0.1",
    "workbox-window": "^7.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@vitejs/plugin-react": "^4.0.1",
    "dotenv": "^16.3.1",
    "globals": "15.12.0",
    "pwa-asset-generator": "^6.3.1",
    "sass": "^1.63.6",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-pwa": "^0.16.7",
    "workbox-cli": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0"
  }
}
